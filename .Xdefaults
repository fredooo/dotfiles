!
! .Xdefaults
!
!

! Dracula Xresources palette
*.foreground: #F8F8F2
*.background: #282A36
*.color0:     #000000
*.color8:     #4D4D4D
*.color1:     #FF5555
*.color9:     #FF6E67
*.color2:     #50FA7B
*.color10:    #5AF78E
*.color3:     #F1FA8C
*.color11:    #F4F99D
*.color4:     #BD93F9
*.color12:    #CAA9FA
*.color5:     #FF79C6
*.color13:    #FF92D0
*.color6:     #8BE9FD
*.color14:    #9AEDFE
*.color7:     #BFBFBF
*.color15:    #E6E6E6

! urxvt ---------------------------------------------------------------

URxvt.saveLines: 10000
URxvt.scrollBar: false
URxvt.background: rgba:0000/0000/0000/D800
URxvt.foreground: white
URxvt.depth: 32
URxvt.font: xft:xos4 Terminus:size=10
URxvt.cursorBlink: true
URxvt.termName: rxvt-256color
URxvt.imLocale: de_DE.UTF-8

URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select,matcher
URxvt.url-select.launcher:  chromium
URxvt.url-select.underline: true
URxvt.matcher.button: 1

URxvt.iso14755: False

URxvt.keysym.M-u: perl:url-select:select_next
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste

URxvt.tabbed.saveLines: 100000
URxvt.tabbed.scrollBar: false
URxvt.tabbed.tabbar-fg: 15
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 15
URxvt.tabbed.tab-bg: 1
URxvt.tabbed.secondaryScroll: true
URxvt.tabbed.font: xft:Terminus:pixelsize=14

URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

! xterm ---------------------------------------------------------------

XTerm*faceName: Terminus:style=Regular:size=10
XTerm*saveLines: 10000
XTerm*locale: true
XTerm*background: rgb:00/00/00/
XTerm*foreground: rgb:ff/ff/ff
XTerm*scrollBar: false
XTerm*eightBitInput: true
XTerm*geometry: 80x25
XTerm*cursorBlink: true
XTerm*utf8: 1

